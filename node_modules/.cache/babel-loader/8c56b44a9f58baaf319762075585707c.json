&appid='+key)\n    axios.get(url+this.search+\"&?units=metric&APPID=\"+key)\n      .then(response => {\n        this.villes = response.data.name;\n        this.temp = response.data.main.temp;\n\n      }\n      )\n      .catch(function (error) {\n    console.log(error);\n  });\n    }\n  }\n}\n\n</script>\n\n<template>\n    <header>\n        <div class=\"search\">\n            <input type=\"text\" class=\"input\" placeholder=\"Type your request\" v-model=\"search\" @keyup.enter=\"getData\"> \n        </div>\n    </header>\n</template>\n\n<style scoped>\n    .search{\n        text-align: center;\n        background-color: white;\n        border-radius: 10px;\n        display: flex;\n        align-items: stretch;\n        flex-wrap: wrap;\n        padding: 10px;\n    }\n\n    .input{\n        text-align: left;\n        width: auto;\n        max-width: auto;\n        flex-grow: 10;\n        border: none;\n    }\n    input:focus, input:hover{\n        border: none;\n        outline: none;\n        box-shadow: none;\n    }\n\n    \n    header{\n        background-color: white;\n        border-radius: 20px 0px 20px 0px;\n        text-align: center;\n        margin: 10px;\n    }\n    header:hover{\n        box-shadow: 1px 1px 2px 0px rgba(0,0,0,0.75);\n    }\n\n\n</style>"],"sourceRoot":"src/components"}]}